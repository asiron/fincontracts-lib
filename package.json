{
  "name": "fincontracts-lib",
  "version": "0.0.1",
  "description": "JavaScript library for interacting with Fincontracts deployed on the Ethereum blockchain",
  "scripts": {
    "compile": "babel --presets es2015,stage-3 -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "xo",
    "docs": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asiron/fincontract-lib.git"
  },
  "author": "asiron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asiron/fincontract-lib/issues"
  },
  "keywords": [
    "ethereum",
    "financial-derivatives",
    "solidity",
    "dapp"
  ],
  "homepage": "https://github.com/asiron/fincontract-lib#readme",
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "esdoc": "^0.5.2",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "mathjs": "^3.9.1",
    "minilog": "^3.1.0",
    "request-promise-native": "^1.0.3"
  },
  "xo": {
    "space": true,
    "rules": {
      "new-cap": 0
    }
  }
}
